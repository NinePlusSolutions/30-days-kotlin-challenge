        app:layout_constraintStart_toStartOf="@id/guideLineVertical0.50" />
    <Button
        android:id="@+id/btnMinus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:text="@string/btnS"
        android:textColor="@color/orange"
        android:textSize="@dimen/_30ssp"
        app:layout_constraintBottom_toTopOf="@id/guideLineHorizontal0.45"
        app:layout_constraintEnd_toEndOf="@id/guideLineVertical1.0"
        app:layout_constraintStart_toEndOf="@id/guideLineVertical0.75" />
    <Button
        android:id="@+id/btnNum3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:text="@string/btn3"
        android:textSize="@dimen/_30ssp"
        app:layout_constraintBottom_toTopOf="@id/guideLineHorizontal0.57"
        app:layout_constraintEnd_toEndOf="@id/guideLineVertical0.75"
        app:layout_constraintStart_toStartOf="@id/guideLineVertical0.50" />
    <Button
        android:id="@+id/btnPlus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:text="@string/btnA"
