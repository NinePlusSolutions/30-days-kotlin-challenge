        app:layout_constraintBottom_toTopOf="@id/guideLineHorizontal0.30"
        app:layout_constraintEnd_toEndOf="@id/guideLineVertical0.50"
        app:layout_constraintStart_toStartOf="@id/guideLineVertical0.25" />
    <Button
        android:id="@+id/btnNum5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:text="@string/btn5"
        android:textSize="@dimen/_30ssp"
        app:layout_constraintBottom_toTopOf="@id/guideLineHorizontal0.45"
        app:layout_constraintEnd_toEndOf="@id/guideLineVertical0.50"
        app:layout_constraintStart_toStartOf="@id/guideLineVertical0.25" />
    <Button
        android:id="@+id/btnNum2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:text="bt"
        android:textSize="@dimen/_30ssp"
        app:layout_constraintBottom_toTopOf="@id/guideLineHorizontal0.57"
        app:layout_constraintEnd_toEndOf="@id/guideLineVertical0.50"
        app:layout_constraintStart_toStartOf="@id/guideLineVertical0.25" />
    <Button
        android:id="@+id/btnNum0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:text="@string/btn0"
        android:textSize="@dimen/_30ssp"
        app:layout_constraintBottom_toTopOf="@id/guideLineHorizontal0.69"
        app:layout_constraintEnd_toEndOf="@id/guideLineVertical0.50"
        app:layout_constraintStart_toStartOf="@id/guideLineVertical0.25" />
    <Button
        android:id="@+id/btnNum9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:text="@string/btn9"
        android:textSize="@dimen/_30ssp"
