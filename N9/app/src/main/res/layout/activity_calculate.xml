        android:id="@+id/guideLineVertical0.75"
        style="@style/guideLineVertical"
        app:layout_constraintGuide_percent="0.75" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLineVertical1.0"
        style="@style/guideLineVertical"
        app:layout_constraintGuide_percent="1.0" />
    <TextView
        android:id="@+id/tvInput"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:text="@string/text_input"
        tools:ignore=""
        android:textAlignment="textEnd"
        android:textColor="@color/black"
        android:textSize="@dimen/_30ssp"
        app:layout_constraintBottom_toBottomOf="@id/guideLineHorizontal0.10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/guideLineHorizontal0.10" />
    <TextView
        android:id="@+id/tvOutPut"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:textAlignment="textEnd"
        android:textColor="@color/black"
        android:textSize="@dimen/_30ssp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvInput" />
    <Button
        android:id="@+id/btnNum7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:text="@string/btn7"
        android:textColor="@color/black"
        android:textSize="@dimen/_30ssp"
        app:layout_constraintBottom_toTopOf="@id/guideLineHorizontal0.30"
        app:layout_constraintEnd_toEndOf="@id/guideLineVertical0.25"
