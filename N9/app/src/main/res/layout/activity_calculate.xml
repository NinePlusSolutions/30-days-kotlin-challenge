        android:id="@+id/btnE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_circle"
        android:paddingHorizontal="@dimen/_10sdp"
        android:text="@string/btnE"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/_30ssp"
        app:layout_constraintBottom_toTopOf="@id/guideLineHorizontal0.69"
        app:layout_constraintEnd_toEndOf="@id/guideLineVertical1.0"
        app:layout_constraintStart_toEndOf="@id/guideLineVertical0.75" />
