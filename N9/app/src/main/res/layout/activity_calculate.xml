<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".calculator2.CalculateActivity">
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLineHorizontal0.10"
        style="@style/guideLineHorizontal"
        app:layout_constraintGuide_percent="0.10" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLineHorizontal0.30"
        style="@style/guideLineHorizontal"
        app:layout_constraintGuide_percent="0.30" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLineHorizontal0.45"
        style="@style/guideLineHorizontal"
        app:layout_constraintGuide_percent="0.45" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLineHorizontal0.57"
        style="@style/guideLineHorizontal"
        app:layout_constraintGuide_percent="0.60" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLineHorizontal0.69"
        style="@style/guideLineHorizontal"
        app:layout_constraintGuide_percent="0.75" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideLineHorizontal0.81"
        style="@style/guideLineHorizontal"
        app:layout_constraintGuide_percent="0.81" />
